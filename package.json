{
    "name": "crm-fullstack",
    "version": "1.0.0",
    "description": "fullstack app with angular and express",
    "main": "index.js",
    "engines": {
        "node": "10.13.0",
        "npm": "6.4.1"
    },
    "scripts": {
        "start": "node index",
        "server": "nodemon index",
        "client-install": "npm install --prefix client",
        "client": "npm run start --prefix client",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run client-install && npm run build --prefix client"
    },
    "keywords": [
        "express",
        "angular"
    ],
    "author": "Alex Gumenyuk",
    "license": "ISC",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "cloudinary": "^1.16.0",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.29.4",
        "mongoose": "^5.6.7",
        "morgan": "^1.9.1",
        "multer": "^1.4.2",
        "passport": "^0.4.0",
        "passport-jwt": "^4.0.0"
    },
    "devDependencies": {
        "concurrently": "^5.0.0",
        "nodemon": "^1.19.1"
    }
}
